{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "removeComments": true,
    "outDir": "dist",
    //"outFile": "./dist/script.js",
    "sourceMap": true,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
